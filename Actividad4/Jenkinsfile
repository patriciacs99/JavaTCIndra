pipeline {
    agent any

    stages {
        stage('Build and Run') {
            steps {
                script {
                    echo 'Compiling and running ControlCalidadEmpleados.java'
                    sh 'javac ./src/main/ControlCalidadEmpleados.java'
                    def output = sh(script: 'java ./src/main/ControlCalidadEmpleados', returnStdout: true).trim()
                    echo "Salida del programa: ${output}"
                }
            }
        }
    }
}
